    "version": "0.2.0",
    "configurations": [
        {
            "name": "Rails server",
            "type": "Ruby",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/bin/rails",
            "args": [
                "server"
            ]
        },
        {
            "name": "Rails server DEBUG",
            "type": "Ruby",
            "request": "launch",
            "showDebuggerOutput": true,
            "useBundler": true,
            "pathToBundler": "/Users/morpheu5/.rvm/gems/ruby-2.5.1/bin/bundle",
            "cwd": "${workspaceRoot}",
            "program": "rdebug-ide",
            "args": [
                "server"
            ]
        }
    ]
